# Device info
genfscon proc /devinfo                                        u:object_r:proc_oppo_devinfo:s0

# Oppo FP
genfscon proc /fp_id                                          u:object_r:proc_oppo_fp:s0

# OppoVersion
genfscon proc /oppoVersion                                    u:object_r:proc_oppoversion:s0

# OppoCustom Sensor
genfscon proc /oppoCustom/Sensor                              u:object_r:proc_oppocustom_sensor:s0

# FEAS
genfscon sysfs /module/perfmgr                                u:object_r:sysfs_perfmgr:s0

genfscon proc /perfmgr                                        u:object_r:proc_perfmgr:s0
